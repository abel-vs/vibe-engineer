            <Data property="Width">
              <Double>120.0</Double>
            </Data>
            <Data property="Height">
              <Double>100.0</Double>
            </Data>
          </Object>
        </Components>
        <Components property="Ports">
          <Object type="Process/MaterialPort" id="unit_reactor_in_1">
            <Data property="Name">
              <String>Feed Inlet</String>
            </Data>
            <Data property="Direction">
              <String>inlet</String>
            </Data>
            <Data property="FlowType">
              <String>material</String>
            </Data>
